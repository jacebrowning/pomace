{
  "files.exclude": {
    ".cache/": true,
    ".venv/": true,
    "*.egg-info": true,
    "pip-wheel-metadata/": true,
    "**/__pycache__": true,
    "**/*.pyc": true,
    "**/.ipynb_checkpoints": true,
    "**/tmp/": true,
    "dist/": true,
    "htmlcov/": true,
    "notebooks/*.yml": true,
    "notebooks/files/": true,
    "notebooks/inventory/": true,
    "prof/": true,
    "site/": true,
    "geckodriver.log": true
  },
  "editor.formatOnSave": true,
  "python.defaultInterpreterPath": ".venv/bin/python",
  "pylint.args": ["--rcfile=.pylint.ini"],
  "cSpell.words": [
    "autopage",
    "chromedriver",
    "connectionpool",
    "cookiecutter",
    "dasherize",
    "eing",
    "esper",
    "geckodriver",
    "getplugin",
    "gitman",
    "Graphviz",
    "grixis",
    "Interactable",
    "linebreak",
    "mkdocs",
    "mockbrowser",
    "mockelement",
    "mockhtml",
    "noninteractive",
    "pluginmanager",
    "proto",
    "pyenv",
    "setenv",
    "showfspath",
    "startfile",
    "tlsv",
    "useragent",
    "USERPROFILE"
  ]
}
